{
	"private": true,
  "name": "<%- name %>",
  "version": "0.0.0",
  "description": "<%- description %>",
  "main": "./src/index.js",
  "jsnext:main": "./src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint src test",
    "test": "npm run lint && nyc mocha test/**/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
		<% if (githubSlug) { %>
    "type": "git",
    "url": "https://github.com/<%- githubSlug %>"
		<% } %>
  },
  "keywords": [
  ],
  "author": {
    "name": "<%- authorName %>",
    "email": "<%- authorEmail %>",
    "website": "<%- authorWebsite %>"
  },
  "license": "",
	<% if (githubSlug) { %>
  "bugs": {
    "url": "https://github.com/<%- githubSlug %>/issues"
  },
  "homepage": "https://github.com/<%- githubSlug %>#readme",
	<% } %>
  "devDependencies": {
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
		"eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.8.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "in-publish": "^2.0.0"
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false
  }
}
